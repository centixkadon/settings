[
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditorInGroup"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditorInGroup"
  },
  {
    "key": "shift+alt+r",
    "command": "revealInExplorer",
    "when": "editorFocus"
  },
  {
    "key": "shift+alt+r",
    "command": "-remote-wsl.revealInExplorer"
  },
  {
    "key": "shift+alt+r",
    "command": "remote-wsl.revealInExplorer",
    "when": "!editorFocus && remoteName == 'wsl'"
  },
]